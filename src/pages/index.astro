---
import Layout from "../layouts/Layout.astro";
import NavigationBar from "../components/Navigation.astro";
import HeroBanner from "../components/Hero.astro";
import CountDown from "../components/CountDown.astro";
import OurStory from "../components/OurStory.astro";
import PhotoGallery from "../components/PhotoGallery.astro";
import Invitation from "../components/Invitation.astro";
---

<Layout title="Welcome to Astro.">
  <main
    class="max-w-auto lg:max-w-[1440px] mx-auto px-5 md:px-3 flex flex-col gap-32 md:gap-40 overflow-hidden"
  >
    <NavigationBar />
    <HeroBanner />
    <CountDown />
    <OurStory />
    <Invitation />
    <PhotoGallery />
  </main>
</Layout>
